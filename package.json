{
  "name": "ciss",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start-ng": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod --build-optimizer",
    "sme": "./node_modules/.bin/source-map-explorer",
    "gen-stats": "ng build --prod --stats-json",
    "view-stats": "webpack-bundle-analyzer dist/publicweb/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docker-up": "docker-compose up -d --build",
    "docker-up-debug": "docker-compose -f docker-compose.debug.yml up -d --build",
    "docker-down": "docker-compose down",
    "start": "PUBLICWEB=./dist/publicweb node -r dotenv/config src/server/index.js",
    "debug": "PUBLICWEB=./dist/publicweb node -r dotenv/config --inspect src/server/index.js"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "^7.3.9",
    "@angular-devkit/schematics": "^7.3.9",
    "@angular/animations": "^7.2.15",
